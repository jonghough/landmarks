{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "deploy": "yarn generate && gh-pages -d dist",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --max-warnings=10",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --quiet --fix",
    "prepare": "husky  && husky install"
  },
  "devDependencies": {
    "@babylonjs/core": "^6.30.0",
    "@babylonjs/inspector": "^6.30.0",
    "@nuxt/devtools": "latest",
    "@types/node": "^20.9.2",
    "eslint": "^9.9.1",
    "gh-pages": "^6.1.1",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.9",
    "nuxt": "^3.8.1",
    "prettier": "^3.3.3",
    "ts-loader": "^9.5.1",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5"
  },
  "dependencies": {
    "@babylonjs/gui": "^6.30.0",
    "@types/geojson": "^7946.0.14",
    "babylonjs": "^6.30.0",
    "babylonjs-gltf2interface": "^6.30.0",
    "babylonjs-gui": "^6.30.0",
    "geojson": "^0.5.0",
    "sass": "^1.69.5",
    "vuetify": "^3.4.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
